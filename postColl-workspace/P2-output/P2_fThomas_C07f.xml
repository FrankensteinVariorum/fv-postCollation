            <seg xml:id="C07f_app2-fThomas_start"/>of our domestic circle <seg xml:id="C07f_app2-fThomas_end"/>
            <seg xml:id="C07f_app3-fThomas_start"/>I <seg xml:id="C07f_app3-fThomas_end"/>
            <seg xml:id="C07f_app4-fThomas_start"/>include <seg xml:id="C07f_app4-fThomas_end"/>
            <seg xml:id="C07f_app5-fThomas_start"/>Henry <seg xml:id="C07f_app5-fThomas_end"/>
            <seg xml:id="C07f_app6-fThomas_start"/>Clerval; <seg xml:id="C07f_app6-fThomas_end"/>
            <seg xml:id="C07f_app7-fThomas_start"/>for he <seg xml:id="C07f_app7-fThomas_end"/>
            <seg xml:id="C07f_app8-fThomas_start"/>was <seg xml:id="C07f_app8-fThomas_end"/>
            <seg xml:id="C07f_app9-fThomas_start"/>constantly <seg xml:id="C07f_app9-fThomas_end"/>
